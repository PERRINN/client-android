<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <include
        android:id="@+id/perrinn_dock"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        layout="@layout/perrinn_dock" />

    <RelativeLayout
        android:layout_alignParentRight="true"
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:layout_above="@+id/perrinn_dock"
        android:layout_alignParentStart="true"
        android:orientation="horizontal"
        android:id="@+id/RelLayoutTeamMembersScreen"
        android:layout_alignParentEnd="true">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1">
                <ImageButton android:text="test1" android:layout_weight="1" ></ImageButton>
                <ImageButton android:text="test2" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1">
                <ImageButton android:text="test1" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test2" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1">
                <ImageButton android:text="test1" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test2" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1">
                <ImageButton android:text="test1" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test2" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight="1">
                <ImageButton android:text="test1" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test2" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
                <ImageButton android:text="test3" android:layout_weight="1"></ImageButton>
            </TableRow>


        </TableLayout>
    </RelativeLayout>

    <TextClock
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/screenClock"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="52dp"
        android:layout_marginTop="58dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="MARKETING"
        android:id="@+id/textViewTeamName"
        android:layout_alignBottom="@+id/screenClock"
        android:layout_alignParentStart="true"
        android:layout_marginStart="37dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Preparing for our next event!"
        android:id="@+id/textViewMessageToTeam"
        android:layout_below="@+id/screenClock"
        android:layout_alignStart="@+id/textViewTeamName" />

</RelativeLayout>